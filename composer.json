{
    "name": "lenderspender/laravel-state-transition-workflow",
    "description": "State transition workflow",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Bert van Hoekelen",
            "email": "bert@magwel.nl"
        }
    ],
    "autoload": {
        "psr-4": {
            "LenderSpender\\StateTransitionWorkflow\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LenderSpender\\StateTransitionWorkflow\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.4.0",
        "illuminate/contracts": "^6.0",
        "illuminate/database": "^6.0",
        "spatie/laravel-queueable-action": "^2.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "lenderspender/laravel-enums": "^0.3.0",
        "mheap/phpunit-github-actions-printer": "^1.2",
        "orchestra/testbench": "^4.4",
        "phpstan/phpstan": "^0.12.4",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "analyse": "vendor/bin/phpstan analyse",
        "fix": "vendor/bin/php-cs-fixer fix"
    }
}
