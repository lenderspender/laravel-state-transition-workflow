{
    "name": "lenderspender/laravel-state-transition-workflow",
    "description": "State transition workflow",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Bert van Hoekelen",
            "email": "bert@magwel.nl"
        }
    ],
    "autoload": {
        "psr-4": {
            "LenderSpender\\StateTransitionWorkflow\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LenderSpender\\StateTransitionWorkflow\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.4.0 | ^8.0",
        "laravel/framework": "^7.0 | ^8.0",
        "spatie/laravel-queueable-action": "^2.10"
    },
    "require-dev": {
        "lenderspender/laravel-enums": "^1.1.0",
        "lenderspender/php-cs-fixer-rules": "dev-master",
        "orchestra/testbench": "^4.0|^5.0",
        "phpstan/phpstan": "^0.12.42",
        "phpunit/phpunit": "^9.3"
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "analyse": "vendor/bin/phpstan analyse",
        "fix": "vendor/bin/php-cs-fixer fix"
    }
}
